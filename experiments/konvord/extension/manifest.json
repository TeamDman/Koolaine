{
    "manifest_version": 3,
    "name": "Konvord",
    "version": "1.0",
    "description": "ChatGPT local integration",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  